<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.sono.dao.LogMessageDao">
 	<select id="findById" parameterType="int" resultType="com.sono.bean.LogMessage">
 		SELECT * FROM LogMessage WHERE id=#{id}
 	</select>
 	<select id="findAll" resultType="com.sono.bean.LogMessage">
 		SELECT * FROM LogMessage
 	</select> 
 </mapper>